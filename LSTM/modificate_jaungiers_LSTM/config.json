{
  "data": {
    "filename": "region.csv",
    "columns": [
      "day of the week",
      "region_DOLNOŚLĄSKIE",
      "region_KUJAWSKO-POMORSKIE",
      "region_LUBELSKIE",
      "region_LUBUSKIE",
      "region_MAZOWIECKIE",
      "region_MAŁOPOLSKIE",
      "region_OPOLSKIE",
      "region_PODKARPACKIE",
      "region_PODLASKIE",
      "region_POLSKA",
      "region_POMORSKIE",
      "region_WARMIŃSKO-MAZURSKIE",
      "region_WIELKOPOLSKIE",
      "region_ZACHODNIOPOMORSKIE",
      "region_ŁÓDZKIE",
      "region_ŚLĄSKIE",
      "region_ŚWIĘTOKRZYSKIE",
      "region_ŚŚ_Gaus_Noise",
      "region_ŚŚ_average",
      "region_ŚŚ_weighted_average",
      "retail_and_recreation_percent_change_from_baseline",
      "grocery_and_pharmacy_percent_change_from_baseline",
      "transit_stations_percent_change_from_baseline",
      "workplaces_percent_change_from_baseline",
      "residential_percent_change_from_baseline",
      "Number_of_people_hospitalized"
    ],
    "sequence_length": 15,
    "train_test_split": 0.85,
    "normalise": false
  },
  "training": {
    "epochs": 10,
    "batch_size": 32
  },
  "model": {
    "loss": "mse",
    "optimizer": "adam",
    "save_dir": "LSTM/modificate_jaungiers_LSTM/saved_models",
    "layers": [
      {
        "type": "lstm",
        "neurons": 100,
        "input_timesteps": 14,
        "input_dim": 8,
        "return_seq": true
      },

      {
        "type": "lstm",
        "neurons": 100,
        "return_seq": false
      },
      {
        "type": "dense",
        "neurons": 1,
        "activation": "linear"
      }
    ]
  }
}
