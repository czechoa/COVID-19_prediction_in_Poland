{
  "data": {
    "filename": "region.csv",
    "columns": [
      "Engaged_respirator"
    ],
    "sequence_length": 15,
    "train_test_split": 0.80,
    "normalise": true
  },
  "training": {
    "epochs": 2,
    "batch_size": 32
  },
  "model": {
    "loss": "mse",
    "optimizer": "adam",
    "save_dir": "LSTM_Neural_Network_for_Time_Series_Prediction/saved_models",
    "layers": [
      {
        "type": "lstm",
        "neurons": 100,
        "input_timesteps": 14,
        "input_dim": 8,
        "return_seq": true
      },
      {
        "type": "lstm",
        "neurons": 100,
        "return_seq": true
      },
      {
        "type": "lstm",
        "neurons": 100,
        "return_seq": false
      },
      {
        "type": "dense",
        "neurons": 1,
        "activation": "linear"
      }
    ]
  }
}
