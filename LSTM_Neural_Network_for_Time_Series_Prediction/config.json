{
  "data": {
    "filename": "data_Poland_to_2021_05_ns.csv",
    "columns": [
        "day of the week",
      "retail_and_recreation_percent_change_from_baseline",
      "grocery_and_pharmacy_percent_change_from_baseline",
      "transit_stations_percent_change_from_baseline",
      "workplaces_percent_change_from_baseline",
      "residential_percent_change_from_baseline",
      "Number_of_people_hospitalized",
      "Engaged_respirator"
    ],
    "sequence_length": 8,
    "train_test_split": 0.80,
    "normalise": false
  },
  "training": {
    "epochs": 2,
    "batch_size": 4
  },
  "model": {
    "loss": "mse",
    "optimizer": "adam",
    "save_dir": "LSTM_Neural_Network_for_Time_Series_Prediction/saved_models",
    "layers": [
      {
        "type": "lstm",
        "neurons": 100,
        "input_timesteps": 7,
        "input_dim": 8,
        "return_seq": true
      },
      {
        "type": "lstm",
        "neurons": 100,
        "return_seq": true
      },
      {
        "type": "lstm",
        "neurons": 100,
        "return_seq": false
      },
      {
        "type": "dense",
        "neurons": 1,
        "activation": "linear"
      }
    ]
  }
}
